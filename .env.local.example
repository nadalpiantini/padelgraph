# Copy this file to .env.local and fill in your actual values
# .env.local is gitignored and should never be committed

# Supabase - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Twilio - Get these from your Twilio console
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_FROM=

# Email - Choose Resend OR Postmark
RESEND_API_KEY=
# POSTMARK_SERVER_TOKEN=
EMAIL_FROM=

# PayPal - Get from https://developer.paypal.com/
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_WEBHOOK_ID=
PAYPAL_MODE=sandbox # sandbox or production
NEXT_PUBLIC_APP_URL=http://localhost:3000 # Your app URL for PayPal redirects
